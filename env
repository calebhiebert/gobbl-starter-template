# Whether or not in-bot developer tools are enabled
ENABLE_DEV_TOOLS=true

# What level of log statements to print
# See https://github.com/calebhiebert/gobbl/wiki/Logging
LOG_LEVEL=50

# Prints the logs with colors, can be funky on some terminals
# See https://github.com/calebhiebert/gobbl/wiki/Logging
LOG_COLORS=true

# The password to connect to redis, can be left blank for development
REDIS_PASSWORD=
